Package: LassoBag
Title: A cart frame work for screen variable or markers
Version: 0.99.6
Authors@R: c(
  person('Yu', 'Sun',   email = "suny226@mail2.sysu.edu.cn", role = c('aut', 'cre'), comment = c(ORCID = '0000-0003-4269-7187')),
